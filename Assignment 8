1. DWORD  1111 1111 1111 1111 1111 1111 1111 1110b = 4,294,967,294

2. SDWORD 1111 1111 1111 1111 1111 1111 1111 1110b = -2

3. DWORD  0000 0000 0000 0000 0000 0000 1111 1110b = 254

4. SDWORD 0000 0000 0000 0000 0000 0000 1111 1110b = 254

	

ExitProcess proto
WriteInt64 proto
Crlf proto
.data

var1 DWORD  11111111111111111111111111111110b
var2 SDWORD 11111111111111111111111111111110b
var3 DWORD  00000000000000000000000011111110b
var4 SDWORD 00000000000000000000000011111110b

.code
main proc
	mov rax, 0
	mov rax, QWORD PTR var1
	call WriteInt64
	call Crlf
	
	movsxd rax, var2
	call WriteInt64
	call Crlf
	
	mov rax, 0
	mov rax, QWORD PTR var3
	movsxd rax, var4
	
	mov ecx, 0
	call ExitProcess
main endp
end
